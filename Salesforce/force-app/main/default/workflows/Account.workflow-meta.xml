    <rules>
        <fullName>Behavioral Health %26 DDS Specialty Group</fullName>
        <actions>
            <name>Check_Behavioral_Health_Dentistry</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2 OR 3 OR 4</booleanFilter>
        <criteriaItems>
            <field>Account.texcellency__DefHC_Hospital_Type__c</field>
            <operation>equals</operation>
            <value>Psychiatric Hospital</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Name</field>
            <operation>contains</operation>
            <value>Psych,Behavioral,Mental,Psychiatric,Dentistry,Dentist,DDS,Smiles,Dental</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Specialties_ULT__c</field>
            <operation>includes</operation>
            <value>Dentist,Psychiatry,Child and Adolescent Psychiatry,Addiction Medicine</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Company_Sub_Type__c</field>
            <operation>contains</operation>
            <value>Dental,Psychiatric Hospital</value>
        </criteriaItems>
        <description>Check the Box for Behavioral Health &amp; Dentistry if the account meets the parameters to be identified as a client or prospect for that group.</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Hospital Based Specialty Group</fullName>
        <actions>
            <name>Hospital_Based_Specialty_Group</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3 OR 4 OR 5)</booleanFilter>
        <criteriaItems>
            <field>Account.Hospital_Based__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Name</field>
            <operation>contains</operation>
            <value>Anesthesia,Emergency,Hospitalist,Radiology,Infectious Disease</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Specialties_ULT__c</field>
            <operation>includes</operation>
            <value>Hospitalist,Emergency Medicine,Radiology,Neonatology,Obstetrics,Pulmonary Critical Care Medicine,CRNA,Critical Care Medicine,Infectious Disease</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Company_Sub_Type__c</field>
            <operation>contains</operation>
            <value>ANES</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.DHC_Main_Specialty__c</field>
            <operation>contains</operation>
            <value>Hospitalist,Radiology,Anesthesiology</value>
        </criteriaItems>
        <description>Check the Box for Hospital Based if the account meets the parameters to be identified as a client or prospect for that group.</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Medestar Client Status Update On Expiration</fullName>
        <actions>
            <name>Update_Medestar_Client_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Account.Medestar_Contract_Expiration__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Updates the status of Medestar Client Status upon expiration of contract (as determined by expiration date field).</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Account.Medestar_Contract_Expiration__c</offsetFromField>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
