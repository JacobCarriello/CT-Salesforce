        <criteriaItems>
            <field>Contact.Requested_Portal_Activation__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Sends email to Salesforce Admin when &quot;Request Portal Activation&quot; checkbox is checked.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Pass Checkbox Notification</fullName>
        <actions>
