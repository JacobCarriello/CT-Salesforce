    <rules>
        <fullName>Blank Primary Email</fullName>
        <actions>
            <name>Update_Primary_email</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Contact.RecordTypeId</field>
            <operation>equals</operation>
            <value>Client,Candidate</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Email</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>If the email field &quot;Primary Email&quot; is blank updates the field to what is in the &quot;Office emiail address&quot;</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Community Portal Activation Request</fullName>
        <actions>
            <name>Community_Portal_Activation_Requeset</name>
            <type>Alert</type>
        </actions>
